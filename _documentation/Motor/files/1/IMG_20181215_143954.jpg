<script>

document.addEventListener("DOMContentLoaded", function() {
    var googleSearchButton = document.querySelector('.google-search-button');
    var xfSearchButton = document.querySelector('.xf-search-button');
    var xfSearchText = document.querySelector('.xf-search-button-text');

	var members = document.querySelector('input[name="c[users]"]');
    var keywords = document.querySelector('input[name="keywords"]');
	if(document.querySelector('select[name="constraints"]') != null)
	{
		var constraints = document.querySelector('select[name="constraints"]');
		constraints.addEventListener('change', isXen);
	}
    var title = document.getElementsByName('c[title_only]')[0]

	function isXen(event){
		var member = (document.activeElement === members);
		if(document.querySelector('select[name="constraints"]') != null)
		{
			var constraints = document.querySelector('select[name="constraints"]');
		}
		else{var constraints={value: ""};}
		if(member || title.checked || constraints.value != ""){
			googleSearchButton.disabled = true;
			googleSearchButton.style.visibility = 'hidden';
			xfSearchText.style.opacity = '1';
			xfSearchButton.classList.add('xf-search-button-only');
		}
		else
		{
			googleSearchButton.disabled = false;
			googleSearchButton.style.visibility = 'visible';
			xfSearchText.style.opacity = '0.5';
			xfSearchButton.classList.remove('xf-search-button-only');
		}
	}
    members.addEventListener('focus', isXen);
    title.onchange = isXen;
    keywords.addEventListener('focus', isXen);

});

</script>

